{
  "name": "befast",
  "version": "0.0.34",
  "description": "The command-line interface for Befa.st",
  "bin": {
    "fast": "./dist/cli.js",
    "befast": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "babel src -d dist && node dist/cli.js",
    "build": "babel src -d dist",
    "build:publish": "npm run build && npm publish",
    "update": "git add . && git commit -m 1 && npm version $(semver $npm_package_version -i patch) && npm uninstall -g befast && npm run build && npm i -g ."
  },
  "engines": {
    "node": ">= 16"
  },
  "homepage": "https://befa.st",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelwitk/befast.git"
  },
  "keywords": [
    "fast",
    "befast",
    "selfhost",
    "self",
    "host",
    "next",
    "nextjs",
    "vercel",
    "netlify",
    "vps",
    "server",
    "heroku",
    "deploy",
    "ssr",
    "static"
  ],
  "author": "michaelwitk",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "xdg-app-paths": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "semver": "^7.6.0"
  }
}
